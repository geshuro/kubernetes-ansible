apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: {{ networkpolicies.ingress.limit_all_trafico.nombre }}
spec:
  podSelector:
    matchLabels:
      {{ networkpolicies.ingress.limit_all_trafico.pod_matchlabels | to_nice_yaml(indent=2) | trim | indent(6) }}
  ingress:
  - from:
      - podSelector:
          matchLabels:
            {{ networkpolicies.ingress.limit_all_trafico.from_pod_matchlabels | to_nice_yaml(indent=2) | indent(12) }}