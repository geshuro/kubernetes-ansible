apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: {{ networkpolicies.ingress.allow_trafico_con_multiple_selectors_app.nombre }}
spec:
  podSelector:
    matchLabels: 
      {{ networkpolicies.ingress.allow_trafico_con_multiple_selectors_app.pod_matchlabels | to_nice_yaml(indent=2) | trim | indent(6)  }}
  ingress:
  - from: 
    {{ networkpolicies.ingress.allow_trafico_con_multiple_selectors_app.from_pod_matchlabels | to_nice_yaml(indent=2)| indent(4) }}