apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  namespace: "{{ networkpolicies.ingress.deny_all_trafico.namespace}}"
  name: "{{ networkpolicies.ingress.deny_all_trafico.nombre }}"
spec:
  podSelector:
    matchLabels:
      {{ networkpolicies.ingress.deny_all_trafico.pod_matchlabels | to_nice_yaml(indent=2) | trim | indent(6) }}
  ingress: []